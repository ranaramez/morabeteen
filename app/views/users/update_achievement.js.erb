<%- @schedules = @response[:schedules] %>
<%- @date = @response[:date] %>
<%- date_id = @date.to_date.strftime("%y%m%d") %>
schedule_data = "<%= escape_javascript(render partial: 'users/schedule-form', locals: {schedules: @schedules, date: @date.to_date, selected_schedule: @response[:selected_schedule], completed_tasks: @response[:completed_task_ids]}) %>";
$("#<%= date_id %>-form .schedule-item").html(schedule_data);
$("#<%= current_user.id %>-acc-score").html(<%= @response[:acc_score] %>);
$("#<%= date_id %>-week-score").html("<%= @response[:week_score] %>");
level = "<%= escape_javascript(render partial: 'users/level_select', locals: {schedules: @schedules, end_date: @schedules.first.end_date, start_date: @schedules.first.start_date, current_level: @response[:current_level]}) %>";
$("#option-level-select").html(level);
$('select').selectpicker();